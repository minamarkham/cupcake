{
	"name": "cupcake",
	"version": "7.0.0",
	"description": "The absolutely delicious, I mean really just scrumptious, personal site for Mina Markham",
	"main": ".eleventy.js",
	"scripts": {
		"🚧": "cross-env ELEVENTY_ENV=development eleventy --serve",
		"🚀": "cross-env ELEVENTY_ENV=production eleventy",
		"🐞": "cross-env DEBUG=Eleventy* ELEVENTY_ENV=development eleventy --serve",
		"dev": "npm run-s 🚧",
		"production": "npm run-s 🚀",
		"debug": "npm run-s 🐞",
		"watch:sass": "sass --watch src/assets/styles:www/assets/styles",
		"build:sass": "sass src/assets/styles:www/assets/styles",
		"start": "npm run-s dev & npm run watch:sass",
		"serve": "npm run-s dev & npm run watch:sass",
		"build": "npm run build:sass & npm run-s production",
		"clean": "rimraf www",
		"prepare": "husky install",
		"lint": "npm-run-all lint:*",
		"lint:fix": "npm-run-all --parallel lint:*:fix",
		"lint:css": "stylelint \"src/**/*.scss\"",
		"lint:css:fix": "stylelint --fix \"src/**/*.scss\"",
		"lint:js": "eslint --cache \"**/*.js\"",
		"lint:js:fix": "eslint --cache --fix \"**/*.js\""
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/minamarkham/cupcake.git"
	},
	"author": {
		"name": "Mina Markham",
		"email": "mina@mina.codes",
		"url": "https://mina.codes/"
	},
	"bugs": {
		"url": "https://github.com/minamarkham/cupcake/issues"
	},
	"homepage": "https://github.com/minamarkham/cupcake#readme",
	"license": "MIT",
	"browserslist": [
		"last 2 versions"
	],
	"dependencies": {
		"clipboard": "^2.0.11",
		"speedlify-score": "^2.0.2"
	},
	"devDependencies": {
		"@11ty/eleventy": "^2.0.0-canary.13",
		"@11ty/eleventy-cache-assets": "^2.3.0",
		"@11ty/eleventy-img": "^2.0.1",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-rss": "^1.1.1",
		"@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
		"@rollup/plugin-commonjs": "^21.0.2",
		"@rollup/plugin-image": "^3.0.1",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@rollup/plugin-replace": "^4.0.0",
		"chalk": "^4.0.0",
		"cross-env": "^7.0.3",
		"eleventy-plugin-metagen": "^1.4.8",
		"eleventy-plugin-rollup": "^1.0.3",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-n": "^15.6.0",
		"eslint-plugin-promise": "^6.1.1",
		"husky": "^8.0.0",
		"lint-staged": "^13.1.0",
		"luxon": "^2.3.0",
		"netlify-cms-app": "^2.9.7",
		"npm-run-all": "^4.1.5",
		"prettier": "2.8.1",
		"rimraf": "^3.0.2",
		"rollup": "^2.69.1",
		"rollup-plugin-terser": "^7.0.2",
		"sass": "^1.57.1",
		"stylelint": "^14.16.1",
		"stylelint-config-prettier": "^9.0.4",
		"stylelint-config-prettier-scss": "0.0.1",
		"stylelint-config-standard": "^29.0.0",
		"stylelint-config-standard-scss": "^6.1.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": "npm run lint:js:fix",
		"*.scss": "npm run lint:css:fix"
	}
}
